{
  "name": "project-datsys_server",
  "version": "1.0.0",
  "description": "Node JS REST API w/ Express JS",
  "main": "server.js",
  "scripts": {
    "start": "node ./server.bundle.js",
    "dev": "nodemon ./server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate-undo": "npx sequelize-cli db:migrate:undo",
    "migrate-reset": "npx sequelize-cli db:migrate:undo:all",
    "migrate-fresh": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed-reset": "npx sequelize-cli db:seed:undo:all",
    "seed-fresh": "npm run seed-reset && npm run seed",
    "build": "webpack"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api"
  ],
  "author": "alvarodwi",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-stream": "^6.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  }
}
